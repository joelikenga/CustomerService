{
  "name": "customer-service-ai",
  "version": "0.2.0",
  "description": "A reusable AI customer service chat widget",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelikenga/CustomerService.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "chat",
    "ai",
    "customer-service",
    "widget"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "framer-motion": "^12.0.0",
    "lucide-react": "^0.470.0"
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}

